<div class="section white">
    <div class="row container">
        <div class="row">
            <div class="col s12" id="high-scores-list">
                <h4 class="center">High Scores</h4>
                <ol>
                    {{#each highScores}}

                    <li data-id="{{id}}">
                        {{'User.username'}} scored {{score}}
                        {{#if status}}
                        <button class="delete-score waves-effect btn-flat" data-id="{{id}}">
                            <i class="material-icons">close</i>
                        </button><br>
                        {{/if}}
                    </li>
                    {{/each}}
                </ol><br>
            </div>
        </div>
    </div>
</div>